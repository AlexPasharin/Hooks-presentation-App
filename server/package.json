{
  "name": "queen-collection",
  "version": "1.0.0",
  "description": "express application to use with my queen collection mysql database",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon -e ts -w ./src -x npm run watch",
    "watch": "ts-node ./index.ts",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix && npm run lint",
    "test": "jest __tests__ --forceExit"
  },
  "author": "Aleksandr Pasharin",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "knex": "^0.15.0",
    "moment": "^2.22.2",
    "mysql": "^2.15.0",
    "promise-mysql": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/node": "^10.0.4",
    "jest": "^23.0.1",
    "nodemon": "^1.17.3",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)test)\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
